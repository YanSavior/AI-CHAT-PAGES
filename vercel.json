{
  "version": 2,
  "builds": [
    {
      "src": "api_server_vercel.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api_server_vercel.py"
    },
    {
      "src": "/(.*)",
      "dest": "api_server_vercel.py"
    }
  ],
  "functions": {
    "api_server_vercel.py": {
      "maxDuration": 30
    }
  }
} 