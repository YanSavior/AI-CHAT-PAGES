{
  "name": "重邮AI问答系统 API 代理",
  "version": "1.0.0",
  "proxies": {
    "deepseek": {
      "target": "https://api.deepseek.com",
      "changeOrigin": true,
      "secure": true,
      "headers": {
        "User-Agent": "CQUPT-AI-Chat/1.0.0"
      }
    },
    "rag": {
      "target": "http://localhost:8000",
      "changeOrigin": true,
      "secure": false,
      "fallback": "local"
    }
  },
  "cors": {
    "origin": true,
    "credentials": true,
    "methods": [
      "GET",
      "POST",
      "PUT",
      "DELETE",
      "OPTIONS"
    ],
    "headers": [
      "Content-Type",
      "Authorization",
      "X-Requested-With"
    ]
  }
}